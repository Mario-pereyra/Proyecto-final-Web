# Configuración de AstroMarket
# Copia este archivo como .env y configura tus valores

# Puerto del servidor (por defecto 3001)
PORT=3001

# Configuración de la base de datos MySQL
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=tu_password_mysql
DB_NAME=db_AstroMarket

# Configuración de archivos (opcional)
# UPLOAD_PATH=uploads
# MAX_FILE_SIZE=5242880

# Configuración de sesiones (opcional)
# SESSION_SECRET=tu_session_secret_aqui

# Configuración de desarrollo (opcional)
# NODE_ENV=development
# DEBUG=true